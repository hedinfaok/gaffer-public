{
  "name": "incremental-testing-example",
  "version": "1.0.0",
  "description": "Real incremental testing with Jest, supertest, and playwright",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "test:lib": "jest tests/unit/lib",
    "test:api": "jest tests/unit/api",
    "test:ui": "jest tests/unit/ui",
    "start": "node src/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.0",
    "jest": "^29.6.0",
    "jest-html-reporters": "^3.1.7",
    "playwright": "^1.36.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "testing",
    "jest",
    "incremental",
    "gaffer-exec"
  ]
}
